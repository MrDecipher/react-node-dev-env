{
  "name": "react-node-dev-env",
  "version": "1.0.0",
  "description": "A development environment for React Applications",
  "main": "index.js",
  "scripts": {
    "build:css": "npm run compile:sass & npm run lint:sass",
    "compile:sass": "node sassCompiler.js",
    "prelaunch": "npm run compile:sass",
    "launch": "node src/server/devServer.js",
    "lint:sass": "sass-lint -c .sass-lint.yml src/srcBuild/styles/**/*.scss -v -q",
    "lint:server": "esw -w src/server/*.js",
    "watch:css": "nodemon -e scss -x \"npm run build:css\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MrDecipher/react-node-dev-env.git"
  },
  "keywords": [],
  "author": "Japheth Ishola",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/MrDecipher/react-node-dev-env/issues"
  },
  "homepage": "https://github.com/MrDecipher/react-node-dev-env#readme",
  "dependencies": {
    "express": "4.15.x",
    "react": "15.6.x",
    "react-dom": "15.6.x",
    "react-router-dom": "4.1.x"
  },
  "devDependencies": {
    "babel-cli": "6.24.x",
    "babel-loader": "7.1.x",
    "babel-preset-env": "1.6.x",
    "babel-preset-es2015": "6.24.x",
    "babel-preset-react": "6.24.x",
    "chalk": "^2.1.0",
    "eslint": "4.1.x",
    "eslint-watch": "3.1.x",
    "node-sass": "^4.5.3",
    "nodemon": "^1.11.0",
    "sass-lint": "^1.10.2"
  }
}
